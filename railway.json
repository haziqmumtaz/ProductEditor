{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "cd frontend && ./railway-build.sh"
  },
  "deploy": {
    "startCommand": "npm run start --workspace=frontend",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
